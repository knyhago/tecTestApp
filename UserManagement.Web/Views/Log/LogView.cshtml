@model UserManagement.Data.Entities.Log


<table class="table table-striped table-bordered">
    <tr><td>Id </td> <td>@Model.Id</td></tr>
    <tr><td>UserId </td> <td>@Model.UserId</td></tr>
    <tr><td>Action </td> <td>@Model.Action</td></tr>
    <tr><td>Details </td> <td>@Model.Details</td></tr>
    <tr><td>PerformedBy </td> <td>@Model.PerformedBy</td></tr>
    <tr><td>Timestamp </td> <td>@Model.Timestamp</td></tr>

    <tr><td></td><td><a asp-action="ViewLogs" asp-controller="Log" asp-route-id="@Model.UserId" class="btn btn-secondary">Back</a></td></tr>
    
</table>